#分割字符串，拼接字符
a="boy. a am i"
b=a.split(" ")
b=list(reversed(b)) #注意这里的list
print(b)
c=" ".join(b)
print(c)

#删除列表中所有指定元素
list_num=[1,2,1,2,3,1,1,1,4]
while(1 in list_num):
    list_num.remove(1)
print(list_num)

#判断一个列表是否包含了另一个列表，只判断元素是否出现，次数不管
a=[2,3,4,5,7,8,1,1]
b=[1,2,3]
c=list(set(a))
print(set(b).issubset(a))
print([item for item in b if item in a]==b)
print(c)
